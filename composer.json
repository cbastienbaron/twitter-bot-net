{
    "name": "cbastienbaron/twitter-bot-net",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.8",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/oauth-subscriber": "^0.4.0",
        "symfony/console": "~4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.2|~4.0",
        "symfony/swiftmailer-bundle": "^3.2|~4.0",
        "symfony/var-dumper": "~4.0",
        "symfony/yaml": "~4.0"
    },
    "require-dev": {
        "symfony/dotenv": "~4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {

        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01CC0PP76HZAF5DDQ6CEX0T0K4",
            "allow-contrib": false
        }
    }
}
